SELECT instructor_person_id, person.name, month_lesson_count FROM (person JOIN (SELECT instructor_person_id,count(*) as month_lesson_c
ount FROM (instructor_lesson JOIN (SELECT id FROM lesson WHERE EXTRACT(MONTH FROM time) = 11 AND EXTRACT(YEAR FROM time) = 2023) ON id = instructor
_lesson.lesson_id) GROUP BY instructor_person_id) ON instructor_person_id = person.id)  WHERE month_lesson_count > 0;
